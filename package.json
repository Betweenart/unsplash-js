{
  "name": "unsplash",
  "version": "0.1.0",
  "description": "A JavaScript client for the Unsplash API.",
  "main": "dist/unsplash.js",
  "scripts": {
    "build": "babel flow src --out-dir dist",
    "build:watch": "babel src --watch --out-dir dist",
    "lint": "eslint src && npm run flow",
    "flow": "flow status",
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "test:ci": "npm run flow && npm run test",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naoufal/unsplash.git"
  },
  "keywords": [
    "unsplash",
    "photos",
    "api",
    "images",
    "splash",
    "free"
  ],
  "author": "Naoufal Kadhom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/naoufal/unsplash/issues"
  },
  "homepage": "https://github.com/naoufal/unsplash#readme",
  "devDependencies": {
    "URIjs": "1.16.1",
    "babel": "5.8.29",
    "babel-core": "5.8.32",
    "babel-eslint": "4.1.3",
    "babel-runtime": "5.8.29",
    "eslint": "1.7.3",
    "expect": "1.12.2",
    "fetch": "0.3.6",
    "flow-bin": "0.18.1",
    "form-data": "0.2.0",
    "isparta": "3.5.3",
    "istanbul": "0.4.0",
    "mocha": "2.3.3",
    "node-fetch": "1.3.3"
  }
}
